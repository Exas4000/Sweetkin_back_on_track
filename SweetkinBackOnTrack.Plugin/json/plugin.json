              "@upg_hostess2",
              "@upg_hostess3"
            ]
          ],
          "icon": "@icon_sally",
          "portrait": "@icon_sally"
        },
        {
          "id": "EvergreenSweetkinChampion",
          "card_data": "@Evergreen",
          "starter_card": "@Sw_WinterSnowstorms",
          "upgrade_tree": [
            [
              "@upg_Trauma1",
              "@upg_Trauma3",
