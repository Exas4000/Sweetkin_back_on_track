      "class": "@ClassSweetkin",
      "targets_room": true,
      "effects": [
        "@SpawnFastfood"
      ],
      "lore_tooltips": [
        {
          "english": "'And this one was the first molded with the help of my eldest brother. I'l say that he did wonderful work.'-Rosette"
        }
      ],
      "pools": [
        "MegaPool"
      ]
    }
  ],
  "characters": [
    {
      "id": "FastFood",
      "size": 1,
      "attack_damage": 0,
      "health": 1,
      "names": {
        "english": "FastFood",
        "chinese": "快餐"
      },
      "character_art": "@FastfoodArt",
      "chatter": "@CTR_morsels",
