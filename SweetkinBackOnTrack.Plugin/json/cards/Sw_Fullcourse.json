{
  "$schema": "https://raw.githubusercontent.com/Monster-Train-2-Modding-Group/Trainworks-Reloaded/refs/heads/main/schemas/schemas/cards.json",
  "cards": [
    {
      "id": "Sw_Fullcourse",
      "names": {
        "english": "Full Course",
        "chinese": "全餐"
      },
      "descriptions": {
        "english": "Apply [effect0.status0.power] <b>Armor</b>, [effect1.status0.power] <b>Rage</b> and <b>Quick</b> to an ally",
        "chinese": "使一个友方单位获得[armor] [effect0.status0.power]，<b>狂怒</b> [effect1.status0.power] 和<b>迅捷</b>。"
      },
      "lore_tooltips": [
        {
          "english": "A wide array of meals seems always at the ready. Exotic food seems the like the norm until they figure out every clan's preference."
        }
      ],
      "rarity": "rare",
      "class": "@ClassSweetkin",
      "card_art": "@CardArt_XI",
      "targets_room": true,
      "targetless": false,
      "pools": [
        "MegaPool"
      ],
      "cost": 3,
      "effects": [
        {
          "id": "@courseArmor"
        },
        {
          "id": "@courseRage"
        },
        {
          "id": "@courseQuick"
        }
      ]
    }
  ],
  "effects": [
    {
      "id": "courseArmor",
      "name": "CardEffectAddStatusEffect",
      "target_mode": "drop_target_character",
      "target_team": "monsters",
      "param_status_effects": [
        {
          "status": "armor",
          "count": 20
        }
      ]
    },
    {
      "id": "courseRage",
      "name": "CardEffectAddStatusEffect",
      "target_mode": "last_targeted_characters",
      "target_team": "monsters",
      "param_status_effects": [
        {
          "status": "buff",
          "count": 10
        }
      ]
    },
    {
      "id": "courseQuick",
      "name": "CardEffectAddStatusEffect",
      "target_mode": "last_targeted_characters",
      "target_team": "monsters",
      "param_status_effects": [
        {
          "status": "ambush",
          "count": 1
        }
      ]
    }
  ],
  "sprites": [
    {
      "id": "CardArt_XI",
      "path": "textures/CardAssets/Card_XI.png"
    }
  ],
  "game_objects": [
    {
      "id": "CardArt_XI",
      "type": "card_art",
      "extensions": {
        "card_art": {
          "sprite": "@CardArt_XI"
        }
      }
    }
  ]
}